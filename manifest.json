{
  "manifest_version": 2,
  "name": "GoogleUrlShortener",
  "description": "copy google url shortener",
  "version": "0.0.1",
  "icons": {
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "options_page": "options.html",
  "browser_action": {
    "default_icon": "img/icon_128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://www.googleapis.com/",
    "tabs",
    "clipboardWrite"
  ]
}
